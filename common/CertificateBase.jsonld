{
  "@context": {
    "@version": "1.1",
    "@protected": true,
    "CertificateBase": {
      "@id": "https://github.com/sunbird-specs/vc-specs#CertificateBase",
      "@context": {
        "id": "@id",
        "@version": 1.1,
        "@protected": true,
        "certificateId": "schema:Text",
        "issuer": "schema:Text",
        "issuedOn": "schema:Text",
        "signedDate": "schema:Text",
        "validFrom": "schema:Text",
        "validUntil": "schema:Text"
      }
    },
    "Subject": {
      "@id": "https://github.com/sunbird-specs/vc-specs#Subject",
      "@context": {
        "id": "@id",
        "@version": 1.1,
        "@protected": true,
        "name": "schema:Text",
        "mobile": "schema:Text",
        "email": "schema:Text"
      }
    }
  }
}