{
  "@context": {
    "@version": "1.1",
    "@protected": true,

    "CertificateBase": {
      "@id": "https://github.com/sunbird-specs/vc-specs#CertificateBase",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",

        "cred": "https://www.w3.org/2018/credentials#",
        "schema": "http://schema.org#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",

        "certificateId": "schema:Text",
        "issuedOn": "cred:issued",
        "signedDate": "xsd:DateTime"
      }
    },

    "Subject": {
      "@id": "https://github.com/sunbird-specs/vc-specs#Subject",
      "@context": {
        "id": "@id",
        "@version": 1.1,
        "@protected": true,

        "sunvc": "https://github.com/sunbird-specs/vc-specs#",
        "identity": "sunvc:identity"
      }
    }
  }
}
