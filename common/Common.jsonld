{
  "@context": {
    "@version": "1.1",
    "@protected": true,
    "identity": {
      "@id": "https://github.com/sunbird-specs/vc-specs#identity",
      "@type": "https://github.com/sunbird-specs/vc-specs#IdentityDetails"
    },
    "Address": {
      "@id": "https://github.com/sunbird-specs/vc-specs#Address",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",

        "plot": "schema:Text",
        "street": "schema:Text",
        "landmark": "schema:Text",
        "locality": "schema:Text",
        "state": "schema:Text",
        "district": "schema:Text",
        "village": "schema:Text",
        "pincode": "schema:Text"
      }
    },
    "ContactDetails": {
      "@id": "https://github.com/sunbird-specs/vc-specs#ContactDetails",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",

        "foaf": "http://xmlns.com/foaf/0.1/",
        "schema": "http://schema.org#",

        "email": "schema:email",
        "mobile": "foaf:phone",
        "address": { "@id": "schema:address", "@type": "https://github.com/sunbird-specs/vc-specs#Address" }
      }
    },
    "IdentityDetails": {
      "@id": "https://github.com/sunbird-specs/vc-specs#IdentityDetails",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",

        "schema": "http://schema.org#",

        "fullName": {
          "@id": "https://github.com/sunbird-specs/vc-specs#fullName",
          "@type": "schema:Text"
        },
        "gender": "schema:gender",
        "dob": "schema:birthDate",
        "identityHolder": {
          "@id": "https://github.com/sunbird-specs/vc-specs#identityHolder",
          "@type": "@id"
        }
      }
    }
  }
}
